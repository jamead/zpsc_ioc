
 

############# Lstats (MSG ID 101) #########################



record(ai, "$(P){$(NO)}Rtos-Uptime-I") {
    field(DESC, "RTOS Uptime")
    field(DTYP, "PSC Reg F32")
    field(INP , "@PSC$(NO) 101 0")
    field(SCAN,"I/O Intr")
