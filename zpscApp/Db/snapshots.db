

record(waveform,"$(P){$(NO)}Chan$(CHAN):$(TYPE):Hdr-Wfm") {
    field(DTYP, "PSC Block F32 In")
    field(INP , "@PSC$(NO) $(MSGID) 0 40")
    field(SCAN,"I/O Intr")
    field(FTVL,"DOUBLE")
    field(NELM,"$(BUFLEN)")
}

